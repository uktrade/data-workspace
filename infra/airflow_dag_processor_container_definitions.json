[
  {
    "command": ${command},
    "environment": [{
      "name": "DB_HOST",
      "value": "${db_host}"
    },{
      "name": "DB_NAME",
      "value": "${db_name}"
    },{
      "name": "DB_PASSWORD",
      "value": "${db_password}"
    },{
      "name": "DB_PORT",
      "value": "${db_port}"
    },{
      "name": "DB_USER",
      "value": "${db_user}"
    },{
      "name": "SECRET_KEY",
      "value": "${secret_key}"
    },{
      "name": "SENTRY_ENVIRONMENT",
      "value": "${sentry_environment}"
    },{
      "name": "AIRFLOW__LOGGING__REMOTE_BASE_LOG_FOLDER",
      "value": "cloudwatch://${cloudwatch_log_group_arn}"
    },{
      "name": "TEAM",
      "value": "${team}"
    },{
      "name": "DAG_SYNC_GITHUB_KEY",
      "value": "${dag_sync_github_key}"
    },{
      "name": "TEAM_SECRET_ID",
      "value": "${team_secret_id}"
    },{
      "name": "S3_IMPORT_DATA_BUCKET",
      "value": "${aws_s3_bucket}"
    }
  ],
    "essential": true,
    "image": "${container_image}",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${log_region}",
        "awslogs-stream-prefix": "${container_name}"
      }
    },
    "networkMode": "awsvpc",
    "memoryReservation": ${memory},
    "cpu": ${cpu},
    "mountPoints" : [],
    "name": "${container_name}",
    "portMappings": [{
        "containerPort": 8080,
        "hostPort": 8080,
        "protocol": "tcp"
    }]
  }
]
